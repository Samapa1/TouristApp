{
  "compilerOptions": {
    "target": "ES2022",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    // "lib": ["ES2022"],
    // "module": "ES2022",

    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noEmit": true, 
    "allowImportingTsExtensions": true
  }
}